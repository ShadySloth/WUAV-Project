<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" stylesheets="@CreateDeviceView.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.CreateDeviceController">
   <children>
      <VBox alignment="CENTER_LEFT" prefHeight="400.0" prefWidth="600.0" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label text="Device">
               <VBox.margin>
                  <Insets bottom="-10.0" left="5.0" />
               </VBox.margin></Label>
            <TextField fx:id="tfDevice" alignment="CENTER" prefHeight="25.0" prefWidth="447.0" />
            <Label text="Username">
               <VBox.margin>
                  <Insets bottom="-10.0" left="5.0" />
               </VBox.margin></Label>
            <TextField fx:id="tfUsername" />
            <Label text="Password">
               <VBox.margin>
                  <Insets bottom="-10.0" left="5.0" />
               </VBox.margin></Label>
            <TextField fx:id="tfPassword" />
            <HBox prefHeight="268.0" prefWidth="527.0" spacing="10.0">
               <children>
                  <Button fx:id="btnCreate" mnemonicParsing="false" onAction="#handleCreateDevice" text="Create" />
                  <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
