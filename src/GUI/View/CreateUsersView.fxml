<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.CreateUsersController">
   <children>
      <TableView fx:id="userList" layoutX="403.0" layoutY="49.0" prefHeight="509.0" prefWidth="493.0">
        <columns>
          <TableColumn fx:id="userscln" prefWidth="492.0" text="Users" />
        </columns>
      </TableView>
      <TextField fx:id="txtNameUser" layoutX="81.0" layoutY="83.0" />
      <TextField fx:id="txtUsernameUser" layoutX="81.0" layoutY="160.0" />
      <PasswordField fx:id="txtPasswordUser" layoutX="81.0" layoutY="233.0" />
      <PasswordField fx:id="txtConfirmPwUser" layoutX="81.0" layoutY="308.0" />
      <Label layoutX="81.0" layoutY="58.0" text="Name" />
      <Label layoutX="81.0" layoutY="137.0" text="Username" />
      <Label fx:id="passwordLabel" layoutX="81.0" layoutY="209.0" text="Password" />
      <Label fx:id="confirmPasswordLabel" layoutX="81.0" layoutY="288.0" text="Confirm Password" />
      <Button fx:id="btnSaveUser" layoutX="191.0" layoutY="534.0" mnemonicParsing="false" onAction="#handleSaveUser" text="Save" />
      <Button fx:id="deleteUser" layoutX="257.0" layoutY="534.0" mnemonicParsing="false" onAction="#handleDeleteUser" text="Delete" />
