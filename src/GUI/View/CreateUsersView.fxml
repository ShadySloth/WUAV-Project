      <RadioButton fx:id="salesChecker" layoutX="81.0" layoutY="371.0" mnemonicParsing="false" text="SalesCheck" />
      <RadioButton fx:id="techChecker" layoutX="81.0" layoutY="408.0" mnemonicParsing="false" text="TechCheck" />
      <RadioButton fx:id="managerChecker" layoutX="81.0" layoutY="444.0" mnemonicParsing="false" text="ManagerCheck" />
      <Button fx:id="btnEditUsers" layoutX="316.0" layoutY="534.0" mnemonicParsing="false" onAction="#handleEditUsers" text="Edit" />
   </children>
</AnchorPane>
